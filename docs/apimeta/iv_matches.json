{
	"title":
		"iv_matches(3)",
	"summary":
		"Check if an interface version implements a required version.",
	"includes":
	[
		"iv/base.h",
		"iv/util.h"
	],
	"libraries":
	[
		"iv_util"
	],
	"synopsis":
	[
		{
			"type": "function",
			"name": "iv_matches",
			"parameters":
			[
				{
					"type": "iv_version_t",
					"name": "version",
					"description": "The version being checked."
				},
				{
					"type": "iv_version_t",
					"name": "req_version",
					"description": "The required version."
				}
			],
			"returns":
			{
				"type": "iv_bool_t",
				"description":
				[
					{
						"type": "ref:constant",
						"name": "IV_TRUE"
					},
					" if compatible, ",
					{
						"type": "ref:constant",
						"name": "IV_FALSE"
					},
					" otherwise."
				]
			},
			"since": "1.0"
		}
	],
	"description":
	[
		"For normal major versions (i.e. not ",
		{
			"type": "text:code",
			"value": "0"
		},
		"), this will check if the major versions are equal and that the checked minor version is greater than or equal to the minor required.",
		null,
		"For the special version ",
		{
			"type": "text:code",
			"value": "0"
		},
		", the minor versions must match."
	]
}
