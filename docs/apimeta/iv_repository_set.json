{
	summary:	'Set an interface entry in a repository.',
	includes:
	[
		'iv/base.h',
		'iv/repository.h'
	],
	synopsis:
	[
		{
			type:	'function',
			name:	'iv_repository_set',
			parameters:
			[
				{
					type:	[ 'iv_repository_t', '*' ],
					name:	'ivr',
					description: 'The repository.'
